<% @todo_lists.each do |list| %>
  <div class="container">
    <div class="row">
      <h2 class="list_title">
        <%= link_to list.title, list %>
      </h2>
      <p class="desc_title">
        <%= list.description %>
      </p>
    </div>
  </div>
<% end %>


<div class="links">
  <%= link_to 'Add new todo list', new_todo_list_path %>
</div>
